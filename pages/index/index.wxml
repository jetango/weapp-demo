<!--pages/index/index.wxml-->
<view class="container">
  <map id="ofoMap"
    latitude="{{latitude}}"
    longitude="{{longitude}}"
    scale="{{scale}}"
    controls="{{controls}}"
    bindcontroltap="bindcontroltap"
    polyline="{{polyline}}"
    markers="{{markers}}"
    bindmarkertap="bindmarkertap"
    show-location
  />
</view>
